<Osejs.Model.Library>
<Osejs.Model.Library.Page>
<Type>LIBRARY_EDITOR</Type>
<Name>calFx</Name>
<Active>true</Active>
<Internal>false</Internal>
<Content>
<Comment><![CDATA[]]></Comment>
<Code><![CDATA[
var r2,r3,f;
// caluclate magnetic field at point xp,yp use Biot-Savart Law Fproportional to dlXr/r^3
function calFx (xp, yp) {
 f=0;
 for(var i=0;i<np;i++){
  for(var j=0;j<nc;j++){
   r2=(xp-xc[j])*(xp-xc[j])+(yp-yc[i])*(yp-yc[i])+zc[i]*zc[i];
   r3=r2*Math.sqrt(r2);
   f-=(yc[i]*(yp-yc[i])-zc[i]*zc[i])/r3;
  }
 }
 return f;
}
]]></Code>
</Content>
</Osejs.Model.Library.Page>
<Osejs.Model.Library.Page>
<Type>LIBRARY_EDITOR</Type>
<Name>calFy</Name>
<Active>true</Active>
<Internal>false</Internal>
<Content>
<Comment><![CDATA[]]></Comment>
<Code><![CDATA[
var r2,r3,f;
function calFy (xp, yp) {
 f=0;
 for(var i=0;i<np;i++){
  for(var j=0;j<nc;j++){
   r2=(xp-xc[j])*(xp-xc[j])+(yp-yc[i])*(yp-yc[i])+zc[i]*zc[i];
   r3=r2*Math.sqrt(r2);
   f+=yc[i]*(xp-xc[j])/r3;
  }
 }
 return f;
}

]]></Code>
</Content>
</Osejs.Model.Library.Page>
<Osejs.Model.Library.Page>
<Type>LIBRARY_EDITOR</Type>
<Name>drag</Name>
<Active>true</Active>
<Internal>false</Internal>
<Content>
<Comment><![CDATA[]]></Comment>
<Code><![CDATA[
function drag () {
// if(x<0)x=0;
// if(y<0)y=0;
xp=xp2=x;
yp=yp2=y;
fx2=fx=calFx(x,y);
fy2=fy=calFy(x,y);
cta=Math.atan2(fy,fx);
}
]]></Code>
</Content>
</Osejs.Model.Library.Page>
